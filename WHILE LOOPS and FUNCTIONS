FILE 1
%While loops
clc, clearvars, close all

start = 100;
stop = 75;

[finalValue, counter] = countFUN(start, stop)






FILE 2
%[out put, output], (input, input)
function [zFinal, counterr] = countFUN(startF, stopF)

counter=0;

while startF>stopF
    startF = startF-1;
    counter = counter + 1;
end

zFinal=startF;
counterr=counter;
end
