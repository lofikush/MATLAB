clc, clearvars, close all

x = linspace (-10,10);
y1 = (-(x-3).^2) +10;
y2 = -(x-3).^2 +15;
y3 = -(x-5).^2 +10;

% figure(1)
% plot(x,y1)
% hold on
% plot(x,y2)
% hold on
% plot(x,y3)

figure(1)
plot(x,y1, '--rs', 'MarkerFaceColor','r','MarkerSize',10)
hold on
plot(x,y2, '--b')
hold on
plot(x,y3, 'y*')

xlabel('x'),ylabel('y'), title('Y vs X- Problem A')
grid on
legend('y1', 'y2', 'y3');
xlim([0,2])
ylim([0,100])
